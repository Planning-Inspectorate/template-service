{% macro pinsHeader(settings) %}
    <header class="govuk-header pins-header {% if settings.isExternal %}pins-header__external{% endif %}" data-module="govuk-header">
        <div class="govuk-header__container govuk-width-container">
            {% if settings.isExtenal %}
                <a href="https://www.gov.uk/government/organisations/planning-inspectorate">
                    {% include "./pins-landscape-logo.njk" %}
                </a>
            {% else %}
                {% include "./pins-landscape-logo.njk" %}
            {% endif %}
        </div>
    </header>
{% endmacro %}
